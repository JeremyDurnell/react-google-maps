{
  "private": false,
  "license": "MIT",
  "version": "0.0.5",
  "name": "@dc0de/react-google-maps",
  "description": "Google Maps integration with React",
  "repository": "https://github.com/dcodeteam/react-google-maps.git",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "engines": {
    "node": ">=8.3.0"
  },
  "typings": "typings/index.d.ts",
  "main": "react-google-maps.cjs.js",
  "module": "react-google-maps.es.js",
  "es2015": "react-google-maps.es2015.js",
  "files": [
    "typings/**",
    "react-google-maps.es.js",
    "react-google-maps.cjs.js",
    "react-google-maps.es2015.js"
  ],
  "scripts": {
    "ci": "yarn lint && yarn dts && yarn cover --ci --runInBand",
    "all": "yarn lint --fix && yarn dts && yarn cover -u",
    "dts": "tsc --noEmit",
    "lint": "js-scripts lint",
    "test": "jest --watch",
    "cover": "jest --coverage",
    "codecov": "codecov -f ./coverage/lcov.info",
    "start": "docz dev",
    "build": "yarn build:bundle && yarn build:typings && yarn build:docs",
    "build:docs": "docz build",
    "build:bundle": "rollup -c ./config/rollup.config.js",
    "build:typings": "rimraf typings && tsc --project tsconfig.typings.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@dc0de/eslint-config": "^0.4.4",
    "@dc0de/eslint-config-base": "^0.4.1",
    "@dc0de/eslint-config-node": "^0.4.4",
    "@dc0de/eslint-config-react": "^0.4.4",
    "@dc0de/js-scripts": "^0.3.1",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-to-json": "^1.5.2",
    "@types/googlemaps": "^3.30.14",
    "@types/jest": "^23.3.5",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "codecov": "^3.1.0",
    "docz": "^0.12.5",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-prettier": "^0.4.0",
    "typescript": "^3.1.3",
    "typescript-eslint-parser": "^20.0.0"
  }
}
