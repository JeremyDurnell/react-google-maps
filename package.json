{
  "private": false,
  "license": "MIT",
  "version": "0.0.5",
  "name": "@dc0de/react-google-maps",
  "description": "Google Maps integration with React",
  "repository": "https://github.com/dcodeteam/react-google-maps.git",
  "author": "Umidbek Karimov <uma.karimov@gmail.com>",
  "engines": {
    "node": ">=8.3.0"
  },
  "typings": "typings/index.d.ts",
  "main": "./react-google-maps.cjs.js",
  "module": "./react-google-maps.es.js",
  "es2015": "./react-google-maps.es2015.js",
  "files": [
    "typings/**",
    "react-google-maps.es.js",
    "react-google-maps.cjs.js",
    "react-google-maps.es2015.js"
  ],
  "scripts": {
    "ci": "yarn lint && yarn dts && yarn cover --ci --runInBand",
    "all": "yarn lint --fix && yarn dts && yarn cover -u",
    "dts": "tsc --noEmit",
    "lint": "js-scripts lint",
    "test": "jest --watch",
    "cover": "jest --coverage",
    "codecov": "codecov -f ./coverage/lcov.info",
    "start": "docz dev",
    "build": "yarn build:es && yarn build:es2015 && yarn build:cjs && yarn build:docs",
    "build:docs": "docz build",
    "build:es": "rollup -c ./config/rollup/rollup.es.js",
    "build:cjs": "rollup -c ./config/rollup/rollup.cjs.js",
    "build:es2015": "rollup -c ./config/rollup/rollup.es2015.js"
  },
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@dc0de/eslint-config": "^0.4.2",
    "@dc0de/eslint-config-base": "^0.4.0",
    "@dc0de/eslint-config-node": "^0.4.2",
    "@dc0de/eslint-config-react": "^0.4.2",
    "@dc0de/js-scripts": "^0.2.1",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-to-json": "^1.5.2",
    "@types/googlemaps": "^3.30.13",
    "@types/jest": "^23.3.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "codecov": "^3.1.0",
    "docz": "^0.12.5",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "rollup": "^0.66.4",
    "rollup-plugin-cleaner": "^0.2.0",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-typescript2": "^0.17.1",
    "ts-jest": "^23.10.3",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^19.0.2"
  }
}
